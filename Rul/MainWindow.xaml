<Window x:Class="Rul.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rul"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Icon="Resources/icon.ico/">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FFABF19C">
            <Button HorizontalAlignment="Left" Content="Выход" Margin="683,11,0,9" Width="107" Background="#FF6BAF5D"/>
            <Image Width="45" Height="45" HorizontalAlignment="Left" Margin="5" Source="Resources/logo.png"/>
        </Grid>
        <Frame x:Name="FraimMain" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
